{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "simple": {
      "format": "%(asctime)s | %(levelname)8s | %(name)s | %(message)s",
      "datefmt": "%d/%m/%Y %H:%M:%S"
    }
  },
  "handlers": {
    "console": {
      "level": "DEBUG",
      "formatter": "simple",
      "class": "logging.StreamHandler"
    }
  },
  "loggers": {
    "root": {
      "handlers": [
        "console"
      ],
      "level": "DEBUG"
    }
  }
}